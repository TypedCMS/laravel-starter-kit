{
    "name": "typedcms/laravel-starter-kit",
    "description": "The a simple starter kit for using TypedCMS with the Laravel framework.",
    "keywords": [
        "TypedCMS",
        "Laravel",
        "Starter Kit",
        "SDK",
        "API"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "George Palmer",
            "email": "george@typedcms.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "kamermans/guzzle-oauth2-subscriber": "^1.0",
        "swisnl/json-api-client": "^1.3"
    },
    "require-dev": {
        "php-http/mock-client": "^1.2",
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.16",
        "phpstan/phpstan": "^0.12.99",
        "nunomaduro/larastan": "^0.7.12"
    },
    "autoload": {
        "psr-4": {
            "TypedCMS\\LaravelStarterKit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TypedCMS\\LaravelStarterKit\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "TypedCMS\\LaravelStarterKit\\Providers\\StarterKitServiceProvider"
            ],
            "dont-discover": [
                "swisnl/json-api-client"
            ]
        }
    }
}
